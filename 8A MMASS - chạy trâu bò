#include<bits/stdc++.h>
#define ll long long
#define lon(x, y) (x > y ? x : y)
#define be(x, y) (x < y ? x : y)
using namespace std;

string s;
ll sum[201], ans = 0;

int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);

    cin >> s;

    int j = 0;
    for(int i = 0; i < s.size(); i++)
    {
        ll tmp;
        if(s[i] == ')')
        {
            if(s[i+1] >= '2' && s[i+1] <= '9')
            {
                i++;
                sum[j] *= 1ll * (s[i] - '0');
            }
            j--;
            sum[j] += sum[j+1];
            continue;
        }
        else if(s[i] == '(')
        {
            j++;
            sum[j] = 0;
            continue;
        }
        else if(s[i] == 'C')
        {
            tmp = 12;
        }
        else if(s[i] == 'H')
        {
            tmp = 1;
        }
        else if(s[i] == 'O')
        {
            tmp = 16;
        }
        if(s[i+1] >= '2' && s[i+1] <= '9')
        {
            i++;
            tmp *= 1ll * (s[i] - '0');
        }
        sum[j] += tmp;
    }
    cout << sum[0];
}
