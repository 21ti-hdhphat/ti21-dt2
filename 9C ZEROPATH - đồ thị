#include<bits/stdc++.h>
using namespace std;

int n, cnt = 0;
vector<int> ans;
vector<bool> b(100001, false);
void dfs(int x)
{
    for(int i = 2; i <= sqrt(x); i++)
    {
        if(x % i == 0)
        {
            int tmp = (i - 1) * (x / i + 1) * 1ll;
            if(b[tmp] == false)
            {
                b[tmp] = true;
                ans.push_back(tmp);
                dfs(tmp);
            }
        }
    }
}

main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);

    cin >> n;
    ans.push_back(0);
    dfs(n);
    sort(ans.begin(), ans.end());
    cout << ans.size() << '\n';
    for(int x : ans) cout << x << ' ';
}
