#include<bits/stdc++.h>
#define ll long long
#define lon(x, y) (x > y ? x : y)
#define be(x, y) (x < y ? x : y)
using namespace std;

int n, m;

int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);

    cin >> n >> m;
    vector<int> a(n+1, 0);
    for(int i = 1; i <= n; i++) cin >> a[i];
    ll sum = 0;
    int j = 1, cnt = 0, res = -1;
    for(int i = 1; i <= n; i++)
    {
        sum += a[i];
        cnt++;
        while(sum > m)
        {
            sum -= a[j++];
            cnt--;
        }
        if(sum == m)
        {
            if(res == -1 || cnt < res) res = cnt;
        }
    }
    cout << res << '\n';
}
